<% provide(:title, '利用者一覧') %>
<h1>利用者一覧</h1>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-hover">
      <thead>
        <tr>
          <th><%= User.name %></th>
        </tr>
      </thead>

    <% @users.each do |user| %>
      <tbody>
        <tr>
        <td><%= link_to user.name, user_path(user) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>    
</div>

  